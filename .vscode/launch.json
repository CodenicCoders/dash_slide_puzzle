  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug_mobile",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--debug"
      ]
    },
    {
      "name": "profile_mobile",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--profile"
      ]
    },
    {
      "name": "debug_web",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--debug",
        "-d",
        "chrome"
      ]
    },
    {
      "name": "profile_web",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "--profile",
        "-d",
        "chrome"
      ]
    }
  ]
}